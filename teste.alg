algoritmo "teste"
// Função :
// Autor :
// Data : 06/05/2019
// Seção de Declarações 
var
     x: inteiro
     y:inteiro


inicio
      Escreva("Insira os valores de x e y :")
      LEIA(x)
      LEIA(y)
      
        Se (x+y >= 10) Entao
        
           Escreva("ok")
        Senao
            x <- x+y
           Escreva(y)
           

       FIMSE

fimalgoritmo
