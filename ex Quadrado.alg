algoritmo "quadrado"
// Função :
// Autor :
// Data : 16/05/2019
// Seção de Declarações 
var
   A, L, P: real


inicio
     Escreva("informe o valor dao quadrado:")
     leia(L)
        A <- L * L
        P <- 4 * L
        Escreva(" O perímetro é :", P)
        Escreva(" A area é :", A)


fimalgoritmo
