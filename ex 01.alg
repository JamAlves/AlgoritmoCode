algoritmo "condicional"
// Função :
// Autor :
// Data : 15/05/2019
// Seção de Declarações 
var
    numero, resto: inteiro
inicio
      leia(numero)
      resto <- numero DIV 2
      resto <- numero - (resto*2)
      Se (resto = 1)Entao
         Escreva("Primeira condição")
      Senao
      
         Escreva("Segunda condição")
        FIMSE

fimalgoritmo
